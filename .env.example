# V-AI Environment Variables

# Public API base URL (New API backend)
NEXT_PUBLIC_API_BASE_URL=https://your-api-domain.com

# New API backend URL (used by admin proxy, defaults to NEXT_PUBLIC_API_BASE_URL)
NEW_API_BASE_URL=https://your-api-domain.com

# Admin credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-me-to-a-strong-password
ADMIN_API_KEY=your-admin-api-key
ADMIN_USER_ID=1

# Database (MySQL)
DATABASE_URL="mysql://user:password@host:3306/database"
